{
    "name": "projek-xyz/ci-startapp",
    "description": "Codeigniter 3 Application Starter Kit",
    "homepage": "http://feryardiant.me/ci-startapp",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "codeigniter", "starter-kit", "project", "skeleton"
    ],
    "authors": [
        {
            "name": "Fery Wardiyanto",
            "email": "hallo@feryardiant.me",
            "homepage": "http://feryardiant.me",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hallo@feryardiant.me"
    },
    "extra": {
        "codeigniter-application-dir": "app",
        "codeigniter-module-dir": "modules",
        "codeigniter-subclass-prefix": "App_"
    },
    "require": {
        "php": ">=5.4.0",
        "codeigniter/framework": "~3.0",
        "projek-xyz/ci-common": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/dbunit": "~1.4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
