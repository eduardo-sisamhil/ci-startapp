{
    "name": "creasi/ci-starter",
    "description": "CodeIgniter-based Project Starter Kit",
    "homepage": "http://feryardiant.me/ci-starter",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "codeigniter", "creasi-app", "starter", "project", "skeleton"
    ],
    "authors": [
        {
            "name": "Fery Wardiyanto",
            "email": "hallo@feryardiant.me",
            "homepage": "http://feryardiant.me",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hallo@feryardiant.me"
    },
    "scripts": {
        "post-install-cmd": [
            "echo 'installed'"
        ]
    },
    "autoload-dev": {
        "classmap": ["tests/units"]
    },
    "extra": {
        "codeigniter-application-dir": "app",
        "codeigniter-module-dir": "modules",
        "codeigniter-subclass-prefix": "App_"
    },
    "require": {
        "php": ">=5.4.0",
        "creasi/common": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/dbunit": "~1.4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../Common"
        },
        {
            "type": "path",
            "url": "../Installer"
        },
        {
            "type": "path",
            "url": "../Module-Admin"
        }
    ]
}
